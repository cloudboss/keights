cloud-init cloud-init/datasources multiselect Ec2
grub-efi-amd64 grub-efi/install_devices string /dev/{{ root_vol }}
grub-efi-amd64 grub-efi/install_devices_empty boolean false
grub-efi-amd64 grub-efi/timeout string 0
grub-efi-amd64 grub2/linux_cmdline_default string elevator=noop console=tty console=ttyS0 swapaccount=1 net.ifnames=0 biosdevname=0 nvme_core.io_timeout=255
locales locales/default_environment_locale string en_US.UTF-8
locales locales/locales_to_be_generated multiselect en_US.UTF-8 UTF-8
openssh-server openssh-server/permit-root-login boolean false
